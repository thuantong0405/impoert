<Window x:Class="autoclick_tmt.Frm.FrmClick"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:autoclick_tmt.Frm"
        mc:Ignorable="d"
        Title="FrmClick" Height="450" Width="800" KeyUp="Window_KeyUp_1">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Left" Margin="122,221,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="txtX" HorizontalAlignment="Left" Height="23" Margin="122,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="txtX_TextChanged"/>
        <TextBox x:Name="txtY" HorizontalAlignment="Left" Height="23" Margin="122,120,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="txtX_TextChanged"/>

    </Grid>
</Window>
